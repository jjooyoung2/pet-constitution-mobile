{
  "expo": {
    "name": "온솔 반려동물 체질 검사",
    "slug": "pet-constitution-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "petconstitution",
    "splash": {
      "image": "./assets/splash-ios.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-splash-screen",
        {
          "android": {
            "image": "./assets/splash.png",
            "imageWidth": 200,
            "backgroundColor": "#ffffff"
          }
        }
      ],
      "expo-web-browser"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.onsolyanghanbong.petconstitution",
      "icon": "./assets/icon-ios.png",
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "체질 진단 결과를 이미지로 저장하기 위해 갤러리 접근 권한이 필요합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "package": "com.onsol.petconstitutionmobile",
      "minSdkVersion": 21,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": false,
          "data": [
            {
              "scheme": "petconstitution",
              "host": "auth"
            },
            {
              "scheme": "petconstitution",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "template": "./public/index.html"
    },
    "extra": {
      "eas": {
        "projectId": "920da7a1-7f23-42ac-ba0d-144194d01305"
      }
    }
  }
}
